package gov.utcourts.common;import java.sql.Connection;/** * This classes handles getting a connection to the data base. to change between straight JDBC and JNDI change the class name */public class DatabaseConnection {		// Static connection for nightly batch processes	public static Connection getJDBCConnectionDistrictStatic() throws Exception {		return JDBCConnect.getConnectionDistrictStatic();	}	// Static connection for nightly batch processes	public static Connection getJDBCConnectionJusticeStatic() throws Exception {		return JDBCConnect.getConnectionJusticeStatic();	}}